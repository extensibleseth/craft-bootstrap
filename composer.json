{
    "name": "extensibleseth/craft-bootstrap",
    "description": "Build your site with the Bootstrap front-end framework.",
    "type": "craft-plugin",
    "version": "5.0.0-beta3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "bootstrap",
        "twitter-bootstrap"
    ],
    "support": {
        "docs": "https://github.com/extensibleseth/craft-bootstrap/blob/v4/README.md",
        "issues": "https://github.com/extensibleseth/craft-bootstrap/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "@Ponies",
            "homepage": "https://iamponies.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.6.0",
        "twbs/bootstrap": "v5.0.0-beta3",
        "components/jquery": "dev-master"
    },
    "autoload": {
        "psr-4": {
          "extensibleseth\\bootstrap\\": "src/"
        }
    },
    "extra": {
        "name": "Bootstrap",
        "handle": "bootstrap",
        "schemaVersion": "0.0.0",
        "changelogUrl": "https://raw.githubusercontent.com/extensibleseth/craft-bootstrap/v4/CHANGELOG.md",
        "class": "extensibleseth\\bootstrap\\Bootstrap"
    }
}
